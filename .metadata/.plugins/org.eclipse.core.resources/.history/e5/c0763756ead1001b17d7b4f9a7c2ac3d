package javatpoint;

public class PerfectSquareOrNot {
	
	public static boolean isPerfectSquare(int a)
	{
		boolean b = false;
		
		double c = Math.sqrt(a);
		if((a*a)==(int)(c*c))
		{
			b=true;
		}
		
		return b;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.prinltn(isPerfectSquare(25));
	}

}
